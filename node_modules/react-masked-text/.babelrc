{
  "env": {
    "test": {
      "presets": [
        "@babel/preset-react",
        "@babel/preset-env"
      ]
    },
    "build": {
       "presets": [
         ["@babel/preset-env", { "modules": false }],
         "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-destructuring",
          "@babel/plugin-transform-parameters",
          "@babel/plugin-proposal-object-rest-spread"
        ]
     }
  }
}